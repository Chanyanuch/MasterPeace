@model IEnumerable<Thammapirom.Models.Background>
@{
    ViewBag.Title = "ประวัติวัด";
}

@section featured {
    <table class="table" style="width: 800px; height: 200px; background-color: rgba(255, 255, 255, 0.90); margin-left: auto; margin-right: auto;">
        <tr>
            <td style="padding-left: 20px; padding-right:20px; padding-top: 30px;">
                <hgroup class="title" style="text-align: center;">
                    <h1 style="color: rgb(25, 25, 25)">@ViewBag.Title</h1>

                </hgroup>
                <p style="text-align:center;"><img src="http://i.imgur.com/n8XSmXd.png" width="241" height="234" /></p>



                <p class="background">

                    @foreach (var background in Model)
                    { 
    
    
                        @background.BackgroundInfo
 
           
                    }

                </p>
            </td>
        </tr>
    </table>

}