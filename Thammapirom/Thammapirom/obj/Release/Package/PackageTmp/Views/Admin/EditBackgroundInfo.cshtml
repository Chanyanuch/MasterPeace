@model Thammapirom.Models.Background

@{
    ViewBag.Title = "EditBackgroundInfo";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@if (Session["LoginStatus"] == "True")
{
    <h2>แก้ไขข้อมูลเกี่ยวกับวัด</h2>
    <table class="table">
        <tr>
            <td>
                @using (Html.BeginForm("EditBackgroundInfo", "Admin"))
                {
                        @Html.ValidationSummary()
                    @Html.EditorForModel()
                    <input type="submit" value="บันทึก" />
                    @Html.ActionLink("ยกเลิกและกลับไปยังหน้าจัดการประวัติ", "BackgroundManaging")
                }
            </td>
        </tr>
    </table>
}

else { }