@model Thammapirom.Models.Background

@{
    ViewBag.Title = "EditBackgroundInfo";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@if (Session["LoginStatus"] == "True")
{
    <h2>EditBackgroundInfo</h2>
    using (Html.BeginForm("EditBackground", "Admin"))
    {
        @Html.EditorForModel()
        <input type="submit" value="Save" />
        @Html.ActionLink("Cancel and return Background Managing Page", "BackgroundManaging")
    }
}
else { }